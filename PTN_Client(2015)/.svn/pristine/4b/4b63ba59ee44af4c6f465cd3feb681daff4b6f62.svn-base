    SELECT s.ID, s.speed, s.NAME, s.BANDWIDTH, s.TYPE, s.ASITEID, s.ZSITEID, s.APORTID, s.ZPORTID, s.ASLOT, s.ZSLOT,sa.CellId AS showaSiteName,
