package com.nms.model.ptn.path.protect;

import java.sql.Connection;
import java.util.List;

import com.nms.db.bean.ptn.path.protect.ProtectRorateInfo;
import com.nms.db.dao.ptn.path.protect.ProtectRorateInfoDao;
import com.nms.model.util.ObjectService;
import com.nms.ui.manager.ExceptionManage;

public class ProtectRorateInfoService extends ObjectService  {
	
	public void setConnection(Connection connection) {
		super.connection = connection;
	}

	public void setPtnuser(String ptnuser) {
		super.ptnuser = ptnuser;
	}
	
	private ProtectRorateInfoDao protectRorateInfoDao = new ProtectRorateInfoDao();
	
	public void insert(ProtectRorateInfo protectRorateInfo)throws Exception{
		try {
			protectRorateInfoDao.insert(protectRorateInfo,connection);
		} catch (Exception e) {
			ExceptionManage.dispose(e,this.getClass());
		}
	}
	
	/**
	 * 删除ProtectRorateInfo
	 * @param protectList
	 * @throws Exception
	 */
	public void delete(ProtectRorateInfo protectRorateInfo)throws Exception{
		 
		try {
			protectRorateInfoDao.delete(protectRorateInfo,connection);
		} catch (Exception e) {
			ExceptionManage.dispose(e,this.getClass());
		}
	}
	
	public void update(ProtectRorateInfo protectRorateInfo)throws Exception{
		try {
			protectRorateInfoDao.update(protectRorateInfo, connection);
		} catch (Exception e) {
			ExceptionManage.dispose(e,this.getClass());
		}
	}
	
	public List<ProtectRorateInfo> queryByProtectRorateInfo(ProtectRorateInfo protectRorateInfo)throws Exception{
		List<ProtectRorateInfo> protectInfoList = null;
		try {
			protectInfoList = protectRorateInfoDao.queryByCondition(protectRorateInfo, connection);
		} catch (Exception e) {
			ExceptionManage.dispose(e,this.getClass());
		}
		
		return protectInfoList;
	}
	
	public List<ProtectRorateInfo> selectAll()throws Exception{
		List<ProtectRorateInfo> protectInfoList = null;
		try {
			protectInfoList = protectRorateInfoDao.queryAll(connection);
		} catch (Exception e) {
			ExceptionManage.dispose(e,this.getClass());
		}
		return protectInfoList;
	}
	 
}
