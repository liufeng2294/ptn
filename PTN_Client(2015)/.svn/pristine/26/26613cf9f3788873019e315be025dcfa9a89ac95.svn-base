package com.nms.ui.ptn.alarm.model;

import twaver.Alarm;

import com.nms.db.enums.EObjectType;

/**
 * 从设备查询的告警信息类
 * 
 * @author lp
 * 
 */
public class AlarmInfo extends Alarm{

	private static final long serialVersionUID = -6233266228685535880L;
	private int id;
	//网元的数据库id
	private int siteId;
	// 网元地址id
	private int siteAddress;
	// 槽位编号
	private int slotNumber;
	// 业务id
	private int objectId;
	// 业务类型，lsp还是tunner
	private EObjectType objectType;
	private int objectType_db;//从数据库里获取值，在service层将type_db转换成枚举类型的type
	//业务名称
	private String objectName;
	//业务类型（int）数据库中显示
	private int intObjectTYpe;
	// 告警代码
	private int alarmCode;
	// 告警等级
	private int alarmLevel;
	// 告警时间
	private String alarmTime;
	private int alarmId;//北向告警匹配ID

	public int getObjectType_db() {
		return objectType_db;
	}

	public void setObjectType_db(int objectType_db) {
		this.objectType_db = objectType_db;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getSiteAddress() {
		return siteAddress;
	}

	public void setSiteAddress(int siteAddress) {
		this.siteAddress = siteAddress;
	}

	public int getIntObjectTYpe() {
		return intObjectTYpe;
	}

	public void setIntObjectTYpe(int intObjectTYpe) {
		this.intObjectTYpe = intObjectTYpe;
	}

	public int getSlotNumber() {
		return slotNumber;
	}

	public void setSlotNumber(int slotNumber) {
		this.slotNumber = slotNumber;
	}

	public int getObjectId() {
		return objectId;
	}

	public void setObjectId(int objectId) {
		this.objectId = objectId;
	}

	public EObjectType getObjectType() {
		return objectType;
	}

	public void setObjectType(EObjectType objectType) {
		this.objectType = objectType;
	}

	public String getObjectName() {
		return objectName;
	}

	public void setObjectName(String objectName) {
		this.objectName = objectName;
	}

	public int getAlarmCode() {
		return alarmCode;
	}

	public void setAlarmCode(int alarmCode) {
		this.alarmCode = alarmCode;
	}

	public int getAlarmLevel() {
		return alarmLevel;
	}

	public void setAlarmLevel(int alarmLevel) {
		this.alarmLevel = alarmLevel;
	}

	public String getAlarmTime() {
		return alarmTime;
	}

	public void setAlarmTime(String alarmTime) {
		this.alarmTime = alarmTime;
	}

	public int getSiteId() {
		return siteId;
	}

	public void setSiteId(int siteId) {
		this.siteId = siteId;
	}

	public int getAlarmId() {
		return alarmId;
	}

	public void setAlarmId(int alarmId) {
		this.alarmId = alarmId;
	}
	
}
