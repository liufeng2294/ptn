      <if test="parentid != null" >
        parentId,
      </if>
      <if test="workip != null" >
        workIP,
      </if>
      <if test="networkid != null" >
        netWorkId,
      </if>
      <if test="groupid != null" >
        groupId,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="id != null" >
        #{id,jdbcType=INTEGER},
      </if>
      <if test="fieldname != null" >
        #{fieldname,jdbcType=VARCHAR},
      </if>
      <if test="fieldx != null" >
        #{fieldx,jdbcType=INTEGER},
      </if>
      <if test="fieldy != null" >
