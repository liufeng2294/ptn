	public int insert(QosMappingAttr qosMappingAttr, Connection conn) throws Exception {
		if (qosMappingAttr == null) {
			throw new Exception("code is null");
		}
		if (conn == null) {
			throw new Exception("connection is null");
		}

		String sql = null;
		PreparedStatement preparedStatement = null;
		int result = 0;

		try {
			sql = "insert into qostable(qosModelId,grade,name,value,siteId,model,direction,color) " + "values(?,?,?,?,?,?,?,?)";
			preparedStatement = conn.prepareStatement(sql);
			preparedStatement.setInt(1, qosMappingAttr.getPhbId());
			preparedStatement.setString(2, qosMappingAttr.getGrade());
			preparedStatement.setString(3, qosMappingAttr.getName());
			preparedStatement.setInt(4, qosMappingAttr.getValue());
			preparedStatement.setInt(5, qosMappingAttr.getSiteId());
			preparedStatement.setInt(6, qosMappingAttr.getModel());
			preparedStatement.setInt(7, qosMappingAttr.getDirection());
			preparedStatement.setInt(8, qosMappingAttr.getColor());
			result = preparedStatement.executeUpdate();
		} catch (Exception e) {
			throw e;
		} finally {
			if (preparedStatement != null) {
				try {
					preparedStatement.close();
				} catch (Exception e) {
					throw e;
				}
				preparedStatement = null;
			}
			sql = null;
		}

		return result;
	}

